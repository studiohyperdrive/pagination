  ],
  "homepage": "https://github.com/studiohyperdrive/pagination",
  "dependencies": {
    "lodash.omit": "4.5.0",
    "qs": "6.9.6"
  },
  "devDependencies": {
    "@studiohyperdrive/commitlint-config": "1.0.0",
    "@types/jest": "26.0.20",
    "@types/lodash.omit": "4.5.6",
    "@types/node": "14.14.21",
    "@types/qs": "6.9.5",
    "@typescript-eslint/eslint-plugin": "4.14.0",
